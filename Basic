import java.util.Scanner;
class Home{
  public static void main(String arg[])
  {
    System.out.println("it is raining outside/Enter true or false");
    Scanner bool = new Scanner(System.in);
    boolean rain = bool.nextBoolean();

    if(rain)
    {
      System.out.print("Take an umberlla");
    }
    else
    {
      System.out.print("Enjoy the sunny day");
    }
  }
}
